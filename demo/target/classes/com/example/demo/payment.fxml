<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.Controllers.FormPaymentController">
   <children>
      <Label layoutX="260.0" layoutY="28.0" text="PAYMENT">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label alignment="CENTER_RIGHT" layoutX="-1.0" layoutY="79.0" prefHeight="27.0" prefWidth="146.0" text="Payment ID:" />
      <TextField fx:id="txtID" layoutX="207.0" layoutY="74.0" />
      <Button fx:id="btnSave" layoutX="14.0" layoutY="342.0" mnemonicParsing="false" onAction="#save" text="Save" />
      <Button fx:id="btnBack" layoutX="95.0" layoutY="342.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <Label alignment="CENTER_RIGHT" layoutX="-1.0" layoutY="137.0" prefHeight="27.0" prefWidth="146.0" text="Payment Type:" />
      <TextField fx:id="txtType" layoutX="208.0" layoutY="138.0" />
   </children>
</AnchorPane>
